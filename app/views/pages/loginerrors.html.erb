    <div class="mt-10 sm:mx-auto sm:w-full sm:max-w-sm">
      <form class="space-y-6" action="#" method="POST">

        <div>
          <label for="email" class="block text-sm font-medium leading-6 text-gray-700">Email address</label>
          <div class="relative mt-2 rounded-md shadow-sm">
            <input type="First name" name="name" id="first name" class="block w-full rounded-md border-0 py-1.5 pr-10 text-red-900 ring-1 ring-inset ring-red-300 placeholder:text-red-300 focus:ring-2 focus:ring-inset focus:ring-red-500 sm:text-sm sm:leading-6" placeholder="" value="" aria-invalid="true" aria-describedby="email-error">
            <div class="pointer-events-none absolute inset-y-0 right-0 flex items-center pr-3">
              <svg class="h-5 w-5 text-red-500" viewBox="0 0 20 20" fill="currentColor" aria-hidden="true">
                <path fill-rule="evenodd" d="M18 10a8 8 0 11-16 0 8 8 0 0116 0zm-8-5a.75.75 0 01.75.75v4.5a.75.75 0 01-1.5 0v-4.5A.75.75 0 0110 5zm0 10a1 1 0 100-2 1 1 0 000 2z" clip-rule="evenodd"></path>
              </svg>
            </div>
          </div>
          <p class="mt-2 text-sm text-red-600" id="email-error">Email address can't be blank.</p>
        </div>

        <div>
          <label for="email" class="block text-sm font-medium leading-6 text-gray-700">Password</label>
          <div class="relative mt-2 rounded-md shadow-sm">
            <input type="First name" name="name" id="first name" class="block w-full rounded-md border-0 py-1.5 pr-10 text-red-900 ring-1 ring-inset ring-red-300 placeholder:text-red-300 focus:ring-2 focus:ring-inset focus:ring-red-500 sm:text-sm sm:leading-6" placeholder="" value="" aria-invalid="true" aria-describedby="email-error">
            <div class="pointer-events-none absolute inset-y-0 right-0 flex items-center pr-3">
              <svg class="h-5 w-5 text-red-500" viewBox="0 0 20 20" fill="currentColor" aria-hidden="true">
                <path fill-rule="evenodd" d="M18 10a8 8 0 11-16 0 8 8 0 0116 0zm-8-5a.75.75 0 01.75.75v4.5a.75.75 0 01-1.5 0v-4.5A.75.75 0 0110 5zm0 10a1 1 0 100-2 1 1 0 000 2z" clip-rule="evenodd"></path>
              </svg>
            </div>
          </div>
          <p class="mt-2 text-sm text-red-600" id="email-error">Password can't be blank.</p>
        </div>

        <div>
          <input type="submit" name="commit" value="Log in" id="sign-up-button" class="bg-orange-600 inline-block w-full px-16 py-3.5 mt-0 mb-0 text-sm font-bold leading-normal text-center text-white align-middle transition-all ease-in border-0 rounded-lg shadow-md cursor-pointer hover:-translate-y-px active:opacity-85 focus-visible:outline focus-visible:outline-2 focus-visible:outline-offset-2 focus-visible:outline-orange-400 tracking-tight-rem bg-150 bg-x-25" data-disable-with="Sign up">
        </div>
      </form>

      <p class="mt-10 text-center text-sm text-gray-500">
        Forgotten your password?
        <!-- space -->
        <a href="/pages/forgotten" class="font-semibold leading-6 text-gray-900 hover:text-gray-500">Reset now</a>
      </p>
